                                {{ this.summary }}
                              </p>
                            {{/if }}
                            {{#if this.villain_summary }}
                              <p>
                                {{#with (lookup ../../../villain.edmunds_ratings.ratings @index) }}
                                  {{ summary }}
                                {{/with }}
                              </p>
                            {{/if }}
                          </div>
                        </div>
                      </div><!-- .b-compare__block-inside -->

