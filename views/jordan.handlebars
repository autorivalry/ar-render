                          </div>
                          <div class="col-xs-4">
                            <div class="b-compare__block-inside-value">
                              <strong>{{ this.score }}</strong>
                            </div>
                          </div>
                          <div class="col-xs-4">
                            <div class="b-compare__block-inside-value">
                              <strong>
                                {{#with (lookup ../../villain.edmunds_ratings.ratings @index) }}
